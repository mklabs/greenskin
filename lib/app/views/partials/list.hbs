<table class="table table-hover table-stripped">
  <thead>
    <tr class="black-muted-bg">
      <th class="align-left">Name</th>
      <th class="align-right">URLs</th>
      <th class="align-right">Next run</th>
      <th class="align-right">Last run started</th>
      <th class="align-right">Last run finished</th>
      <th class="align-left">Last results</th>
      <th class="align-left">Frequency</th>
    </tr>
  </thead>
  <tbody>
  {{#each jobs}}
    <tr>
      <td class="align-left">{{ this.name }}</td>
      <td class="align-right">
        {{#each this.data.urls}}
          <a href="{{ . }}">{{ . }}</a> <br />
        {{/each}}
      </td>
      <td class="align-right">{{fromNow this.nextRunAt }}</td>
      <td class="align-right">{{fromNow this.lastRunAt }}</td>
      <td class="align-right">{{fromNow this.lastFinishedAt }}</td>
      <td class="align-left">{{{results this.lastRunResults }}}</td>
      <td class="align-left">{{ this.data.frequency }}</td>
    </tr>
  {{/each}}
  </tbody>
</table>

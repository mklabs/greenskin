{
  "server": {
    "accessLog": "/var/log/nginx/access.log",
    "workers": 10,
    "maxSockets": 100,
    "deadBackendTTL": 30,
    "tcpTimeout": 30,
    "retryOnError": 3,
    "deadBackendOn500": true,
    "httpKeepAlive": false,
  },
  "http": {
    "port": 80,
    "bind": "127.0.0.1",
  },
  "driver": "redis://127.0.0.1:6379"
}

{
  "title": "$name Dashboard",
  "services": {
    "filter": {
      "list": [{
        "type": "filter",
        "name": "ScoreURL",
        "query": "stats.gauges.$name.scores.http.*",
        "includeAll": true,
        "options": [{
          "text": "All",
          "value": "*"
        }],
        "current": {
          "text": "All",
          "value": "*"
        }
      }, {
        "type": "filter",
        "name": "TimingURL",
        "query": "stats.gauges.$name.timings.firefox.http.*",
        "includeAll": true,
        "options": [{
          "text": "All",
          "value": "*"
        }],
        "current": {
          "text": "All",
          "value": "*"
        }
      }],
      "time": {
        "from": "now-6h",
        "to": "now"
      }
    }
  },


  "rows": [
  {
    "title": "Description",
    "height": "150px",
    "editable": true,
    "collapse": false,
    "collapsable": true,
    "panels": [
      {
        "error": false,
        "span": 12,
        "editable": true,
        "type": "text",
        "loadingEditor": false,
        "mode": "markdown",
        "content": "####Score and metrics.\n\nYou'll find below only a fraction of the scores stored in Graphite.\n\n You can see them all with stats.gauges.harsummary.scores.http.example-com.*",
        "style": {},
        "title": ""
      }
    ],
    "notice": false
  },
  {
    "title": "Some scores",
    "height": "150px",
    "editable": true,
    "collapse": false,
    "collapsable": true,
    "notice": false,
    "panels": [{
      "span": 4,
      "title": "criticalpath score",
      "type": "graphite",
      "lines": true,
      "legend": false,
      "pointradius": 1,
      "stack": false,
      "points": true,
      "targets": [{
        "target": "stats.gauges.$name.scores.http.[[ScoreURL]].criticalpath"
      }]
    }, {
      "span": 4,
      "title": "totalrequests score",
      "type": "graphite",
      "lines": true,
      "pointradius": 1,
      "stack": false,
      "legend": false,
      "points": true,
      "targets": [{
        "target": "stats.gauges.$name.scores.http.[[ScoreURL]].totalrequests"
      }]
    }, {
      "span": 4,
      "title": "spof score",
      "lines": true,
      "stack": false,
      "pointradius": 1,
      "legend": false,
      "points": true,
      "type": "graphite",
      "targets": [{
        "target": "stats.gauges.$name.scores.http.[[ScoreURL]].spof"
      }]
    }]
  }, {
    "title": "All scores",
    "height": "450px",
    "collapse": true,
    "collapsable": true,
    "panels": [{
      "span": 12,
      "title": "All scores",
      "type": "graphite",
      "stack": false,
      "pointradius": 1,
      "lines": true,
      "points": true,
      "targets": [{
        "target": "stats.gauges.$name.scores.http.[[ScoreURL]].*"
      }]
    }]
  }, {
    "title": "All timings",
    "height": "450px",
    "panels": [{
      "span": 6,
      "title": "All timings Firefox",
      "type": "graphite",
      "stack": false,
      "pointradius": 1,
      "lines": true,
      "legend": false,
      "points": true,
      "targets": [{
        "target": "stats.gauges.$name.timings.firefox.http.[[TimingURL]].*.avg"
      }]
    }, {
      "span": 6,
      "title": "All timings Chrome",
      "type": "graphite",
      "stack": false,
      "lines": true,
      "pointradius": 1,
      "legend": false,
      "points": true,
      "targets": [{
        "target": "stats.gauges.$name.timings.chrome.http.[[TimingURL]].*.avg"
      }]
    }]
  }],

  "editable": true,
  "failover": false,
  "panel_hints": true,

  "style": "dark",

  "pulldowns": [
    {
      "type": "filtering",
      "collapse": false,
      "notice": true,
      "enable": true
    }
  ],

  "nav": [
    {
      "type": "timepicker",
      "collapse": false,
      "notice": false,
      "enable": true,
      "status": "Stable",
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ],
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "now": true
    }
  ]
}
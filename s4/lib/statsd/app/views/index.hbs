

<div class="js-statsd">

  <form class="form js-form">
    <fieldset>
      <legend>
        <h2>
          Statsd metrics
          <a class="small" href="/statsd/sets">
            (sets)
          </a>
          <input type="submit" class="btn btn-default right" value="submit">
        </h2>
      </legend>

      <div class="row">
        <div class="col-sm-10">
          <input class="form-control js-input"
            name="q" type="text"
            placeholder="Metric key (blah.blah.*.blah)">
        </div>
        <div class="col-sm-2">
          <input class="form-control js-from"
            name="from" type="text"
            placeholder="7d">
        </div>
      </div>
    </fieldset>
  </form>

  <p class="description">
    <a href="#" class="js-url"></a>
  </p>

  <div class="statsd-metrics js-metrics">


  </div>
</div>

{{#extend 'scripts'}}
<script src="/statsd/app.js"></script>
{{/extend}}

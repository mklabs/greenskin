<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>{{ title }}</title>
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css">
  <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/codemirror/4.0.3/codemirror.min.css">
  <link rel="stylesheet" href="//codemirror.net/addon/hint/show-hint.css">

  <link rel="stylesheet" href="http://codemirror.net/lib/codemirror.css">
  <link rel="stylesheet" href="http://codemirror.net/addon/hint/show-hint.css">

  <link rel="stylesheet" href="/stylesheets/style.css">
  <link rel="stylesheet" href="/vendor/har-view/har-viewer.css">
  <link rel="stylesheet" href="/vendor/har-view/jquery-ui.css">
  <link rel="stylesheet" href="/vendor/select2/select2.css">
  <link rel="stylesheet" href="/vendor/animate.css">
  {{{ moreCss }}
</head>
<body class="app">
  <nav class="navbar navbar-default navbar-static-top app-top" role="navigation">
  	<div class="container-fluid">
  	  <div class="navbar-header navbar-brand">
        <a href="/" class="navbar-link">
          <span class="glyphicon glyphicon-home"></span>
        </a>
        <span class="small">/</span>
        <span class="page-title">
          {{ title }}
        </span>
      </div>

      <ul class="nav navbar-nav navbar-right">
        <li><a href="/jenkins">Jenkins</a></li>
        {{#kue}}<li><a href="/kue">Kue</a></li>{{/kue}}
      </ul>
  	</div>
  </nav>

  {{#showqueue}}
  <div class="js-queue app-left">
    <form class="search-box">
      <input class="text-field form-control" placeholder="Search all repositories" type="text">
    </form>


    <ul class="nav nav-tabs">
      <li class="active"><a href="/edit/{{ job.name }}">Build Queue</a></li>
      <li><a href="#" class="is-hidden">Search</a></li>
      
    </ul>

    <a href="#" class="list-group-item js-row-template is-hidden">
      <h4 class="list-group-item-heading title js-title">
        List group item heading
      </h4>
      <p class="list-group-item-text js-why">Donec id elit non mi porta gravida at eget metus. Maecenas sed diam eget risus varius blandit.</p>

      <p class="summary">
        <span class="duration_label">Duration:</span>
        <abbr class="duration js-duration" title=""></abbr>
      </p>

      <p class="summary">
        <span class="finished-icon"></span><span class="finished_at_label">Finished:</span>
        <abbr class="duration js-fininishedat" title=""></abbr>
      </p>
    </a>

    <div class="list-group js-list">
      
    </div>
  </div>
  {{/showqueue}}
  <div class="app-main container-fluid">
    {{{ yield }}}
  </div>

  <script src="/socket.io/socket.io.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/underscore.js/1.6.0/underscore-min.js"></script>
  <script src="https://code.jquery.com/jquery-2.1.0.min.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/codemirror/4.0.3/codemirror.min.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/codemirror/4.0.3/mode/xml/xml.min.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/codemirror/4.0.3/mode/javascript/javascript.js"></script>
  <script src="//codemirror.net/mode/gherkin/gherkin.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/codemirror/4.0.3/addon/hint/show-hint.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.6.0/moment.min.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/highcharts/3.0.10/highcharts.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/screenfull.js/1.0.4/screenfull.min.js"></script>

  <script src="/vendor/jquery-cron.js"></script>
  <script src="/vendor/jsonlint.js"></script>
  <script src="/vendor/select2/select2.js"></script>
  <script src="/vendor/mustache.js"></script>
  <script src="/vendor/humanize.min.js"></script>
  <script src="/vendor/jquery-ui.min.js"></script>
  <script src="/vendor/har-viewer.js"></script>
  <script src="/vendor/ansiparse.js"></script>
  <script src="/vendor/bootstrap-modal.js"></script>

  <script src="/js/pages/queue.js"></script>
  <script src="/js/pages/create.js"></script>
  <script src="/js/pages/create-feature.js"></script>
  <script src="/js/pages/build.js"></script>
  <script src="/js/pages/feature.js"></script>
  <script src="/js/pages/step.js"></script>
  <script src="/js/pages/home.js"></script>
  <script src="/js/pages/highcharts-view.js"></script>

  {{{ moreJs }}
</body>
</html>
